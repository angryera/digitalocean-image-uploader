# DigitalOcean Spaces Configuration
# Copy this file to .env and fill in your credentials

# Your DigitalOcean Spaces access key
SPACES_ACCESS_KEY=your_access_key_here

# Your DigitalOcean Spaces secret key
SPACES_SECRET_KEY=your_secret_key_here

# The name of your Spaces bucket
SPACES_BUCKET=your_bucket_name

# The region where your bucket is located (e.g., nyc3, sfo2, sfo3, ams3, sgp1, fra1)
SPACES_REGION=nyc3

# The endpoint URL for your region
SPACES_ENDPOINT=https://nyc3.digitaloceanspaces.com

# Optional: Set to 'public-read' to make uploaded files publicly accessible, or 'private' to keep them private
SPACES_ACL=public-read

